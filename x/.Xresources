! red
*.color1:       #ad4343
*.color9:       #e64c4c

! green
*.color2:       #4e9a06
*.color10:      #5fa41b

! yellow
*.color3:       #c4a000
*.color11:      #fce94f

! blue
*.color4:       #3465a4
*.color12:      #729fcf

! magenta
*.color5:       #75507b
*.color13:      #ad7fa8

! cyan
