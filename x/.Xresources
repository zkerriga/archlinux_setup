Xft.dpi: 120
Xft.antialias: 1
Xft.hinting: 1
Xft.rgba: rgb
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

URxvt*scrollBar: false
URxvt*font: xft:Monaco:pixelsize=18:minspace=False
URxvt*letterSpace: 1
